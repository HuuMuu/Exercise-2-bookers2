<h1>New book</h1>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
    <%= f.text_field :title %>
  <h4>Opinion</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Create Book' %>
<% end %>

<h1>User</h1>
<% @users.each do |user| %>
  <div>
      <%= image_tag user.get_image %>
      <%= user.name %>
      <%= link_to 'show', user_path(user.id) %>
  </div>
<% end %>